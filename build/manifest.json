{
  "name": "Pihh's screen recorder",
  "description": "A free chrome extension for screen recording :) ",
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "32": "img/logo-34.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.chunk-4ee47c15.4537c38a.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "icons/icon16.png",
        "img/logo-34.png",
        "icons/icon48.png",
        "icons/icon128.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/chunk-4ee47c15.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "permissions": []
}